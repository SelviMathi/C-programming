1.#include<stdio.h>
void main()
{

    char str[50]="RVSCCT";
    char alphabets[123]={0};
    int index=0;
    while(str[index] !='\0')
        {
        alphabets[str[index]]++;
        index++;
        }
    for(int i=0;i<123;i++)
    {
        if(alphabets[i]!=0)
        printf("%c--%d time\n",i,alphabets[i]);
        
    }
    
}
